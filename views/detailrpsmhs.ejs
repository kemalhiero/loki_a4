<%- include('layout/atas', {judul: 'Detail RPS'}) %>

  <div class="page-heading">
    <div class="page-title">
      <div class="row">
        <div class="col-12 col-md-6 order-md-1 order-last">
          <h3>Detail RPS</h3>
          <!-- <p class="text-subtitle text-muted">Give textual form controls like input upgrade with custom styles,
                          sizing, focus states, and more.</p> -->
        </div>
      </div>
      <div class="button">
        
        <br />
        <button type="button" class="btn btn-primary rounded-pill" onClick="window.open('/mahasiswa/printrps')"><i class="fa-solid fa-print" onClick="window.open('/print')"></i> Print</button>
        
      </div>
      <br />
    </div>

    <!-- Horizontal Input start -->
    <section id="horizontal-input">
      <div class="row">
        <div class="col-md-12">
          <div class="card">
            <div class="card-header">
              <h4 class="card-title">RPS - <%= rps.name %></h4>
            </div>

            <div class="card-body">
              <div class="row">
                <div class="col-md-6">
                  <div class="form-group row align-items-center">
                    <div class="col-lg-4 col-3">
                      <label class="col-form-label"><h6>Kode Matkul</h6></label>
                    </div>
                    <div class="col-lg-5 col-9">
                      <p class="form-control-static" id="staticInput"><%= rps.code %></p>
                    </div>
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="form-group row align-items-center">
                    <div class="col-lg-4 col-3">
                      <label class="col-form-label"><h6>Semester</h6></label>
                    </div>
                    <div class="col-lg-6 col-9">
                      <p class="form-control-static" id="staticInput"><%= rps.semester %></p>
                    </div>
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="form-group row align-items-center">
                    <div class="col-lg-4 col-3">
                      <label class="col-form-label"><h6>Nama Matkul</h6></label>
                    </div>
                    <div class="col-lg-5 col-9">
                      <p class="form-control-static" id="staticInput"><%= rps.name %></p>
                    </div>
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="form-group row align-items-center">
                    <div class="col-lg-4 col-3">
                      <label class="col-form-label"><h6>SKS</h6></label>
                    </div>
                    <div class="col-lg-6 col-9">
                      <p class="form-control-static" id="staticInput"><%= rps.credit %></p>
                    </div>
                  </div>
                </div>
                <br /><br />
              </div>
            </div>

            <div class="card-body">
              <div class="form-group mb-3">
                <label for="exampleFormControlTextarea1" class="form-label"><h6>Deskripsi</h6></label>
                <textarea class="form-control" id="exampleFormControlTextarea1" rows="3" readonly="readonly"><%= rps.description %></textarea>
              </div>
            </div>

          </div>

          <div class="card p-3">
            <div class="card-header">
                <h4 class="card-title">CP Program Studi</h4>
            </div>
            <div class="card-content">
                <!-- table hover -->
                <div class="table-responsive">
                    <table class="table table-hover mb-0">
                        <thead>
                            <tr>
                                <th>Kode</th>
                                <th>Nama</th>
                            </tr>
                        </thead>
                        <tbody>
                          <% cpps.forEach(function(cpl, index) { %>
                            <tr>
                                <td><%= cpl.code %></td>
                                <td><%= cpl.name %></td>
                              </tr>
                          <% }) %>
                        </tbody>
                    </table>
                </div>
            </div>
          </div>

          <div class="card p-3">
            <div class="card-header">
                <h4 class="card-title">CP Mata Kuliah</h4>
            </div>
            <div class="card-content">
                <!-- table hover -->
                <div class="table-responsive">
                    <table class="table table-hover mb-0">
                        <thead>
                            <tr>
                                <th>Kode</th>
                                <th>Nama</th>
                            </tr>
                        </thead>
                        <tbody>
                          <% cpps.forEach(function(cpl, index) { %>
                            <tr>
                                <td></td>
                                <td></td>
                              </tr>
                          <% }) %>
                        </tbody>
                    </table>
                </div>
            </div>
          </div>

          <div class="card p-3">
            <div class="card-header">
                <h4 class="card-title">Referensi</h4>
            </div>
            <div class="card-content">
                <!-- table hover -->
                <div class="table-responsive">
                    <table class="table table-hover mb-0">
                        <thead>
                            <tr>
                                <th>Kode</th>
                                <th>Nama</th>
                            </tr>
                        </thead>
                        <tbody>
                          <!-- <% cpps.forEach(function(cpl, index) { %> -->
                            <tr>
                                <td></td>
                                <td></td>
                              </tr>
                          <!-- <% }) %> -->
                        </tbody>
                    </table>
                </div>
            </div>
          </div>


        </div>
      </div>
    </section>
    <!-- Horizontal Input end -->
  </div>

<%- include('layout/bawah') %> 
