                                    <div class="tab-content text-justify">
                                        <div class="tab-pane fade show active" id="list-cpmk" role="tabpanel"
                                            aria-labelledby="list-cpmk-list"><br>
                                            <%- include ('kelolarps/kelolacpmk') %> 
                                        </div>
                                        <div class="tab-pane fade" id="list-referensi" role="tabpanel"
                                            aria-labelledby="list-referensi-list"><br>
                                            <%- include ('kelolarps/kelolareferensi') %> 
                                        </div>
                                        <div class="tab-pane fade" id="list-penilaian" role="tabpanel"
