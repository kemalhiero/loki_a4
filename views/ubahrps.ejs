                                <div>
                                    <div class="list-group list-group-horizontal-sm mb-1 text-center" role="tablist">
                                        <a class="list-group-item list-group-item-action active" id="list-cpmk-list"
                                            data-bs-toggle="list" href="#list-cpmk" role="tab">CPMK</a>
                                        <a class="list-group-item list-group-item-action" id="list-referensi-list"
                                            data-bs-toggle="list" href="#list-referensi" role="tab">Referensi</a>
